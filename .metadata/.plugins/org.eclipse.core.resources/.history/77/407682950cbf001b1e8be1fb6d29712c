package com.java.org;

public class Method_Overloading {  // class name
	
	private void stuId(int id) { // method  // int parameters  // id obj

		System.out.println("Student ID is " + id);
		
	}

	private void stuName(String name) {  // String parametrs // name object
		
		System.out.println("Student Name is " + name);
	}
	
	private void stuGrade(String name, String grade) {
		
		System.out.println("Student Name & Grade is " + name + "," + grade);

	}
	
	public static void main(String[] args) {  // main method 
		Method_Overloading mo = new Method_Overloading();  // object creation = classname objectname = new classname 
		mo.stuId(1246);
		mo.stuName("Suba");
		mo.stuId(1239);
		mo.stuName("Seetha");
		mo.stuGrade("Mani", "A");
		
	
	}
	
}

